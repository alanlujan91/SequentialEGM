top=x_max, num=n_test).reshape(-1, 1)\ny_test = f(X_test)\n\nX_train = (rng.rand(n_train) * x_max).reshape(-1, 1)\ny_train = f(X_train) + s * rng.randn(n_train).reshape(-1, 1)\nX_train.shape, y_train.shape","key":"gOcwnkgwcU"},{"type":"output","id":"nD_EYZMvnqgym3v6wJdZ4","data":[{"output_type":"execute_result","execution_count":3,"metadata":{},"data":{"text/plain":{"content":"((10, 1), (10, 1))","content_type":"text/plain"}}}],"key":"FlVoYgS9RK"}],"data":{"type":"notebook-code","execution":{"iopub.execute_input":"2023-07-20T14:17:10.589119Z","iopub.status.busy":"2023-07-20T14:17:10.589119Z","iopub.status.idle":"2023-07-20T14:17:10.602454Z","shell.execute_reply":"2023-07-20T14:17:10.602156Z"}},"key":"EFr3YXT3GI"},{"type":"block","children":[{"type":"code","lang":"python","executable":true,"value":"fig, ax = plt.subplots(figsize=(15, 5))\nax.plot(X_test, y_test, label=latex_func, linestyle=\"dotted\")\nax.scatter(X_train, y_train, label=\"Randomly sampled points\", s=100)\nax.legend()\nax.set_xlabel(\"$x$\")\nax.set_ylabel(\"$f(x)$\")\nax.set_title(\"True function and randomly sampled points\")\nfig.tight_layout()\nfig.savefig(figures_path + \"GPR_True_Function.svg\")\nfig.savefig(figures_path + \"GPR_True_Function.pdf\")","key":"CBEtejBSWq"},{"type":"output","id":"lJM5IIQc6qxZyJS_37oZ6","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"27bc81c09bd67123fe4c611b222b9cad","path":"/SequentialEGM/build/27bc81c09bd67123fe4c611b222b9cad.png"},"text/plain":{"content":"<Figure size 1500x500 with 1 Axes>","content_type":"text/plain"}}}],"key":"yf9Jx2TF37"}],"data":{"type":"notebook-code","execution":{"iopub.execute_input":"2023-07-20T14:17:10.604969Z","iopub.status.busy":"2023-07-20T14:17:10.603964Z","iopub.status.idle":"2023-07-20T14:17:11.212207Z","shell.execute_reply":"2023-07-20T14:17:11.211983Z"}},"key":"lml7XaifiF"},{"type":"block","children":[{"type":"code","lang":"python","executable":true,"value":"# Define the kernel\ndef kernel(a, b):\n    \"\"\"GP squared exponential kernel function\"\"\"\n    kernelParameter_l = 0.5\n    kernelParameter_sigma = 1.0\n    sqdist = (\n        np.sum(a**2, axis=1).reshape(-1, 1) + np.sum(b**2, 1) - 2 * np.dot(a, b.T)\n    )\n    # np.sum( ,axis=1) means adding all elements columnly; .reshap(-1, 1) add one dimension to make (n,) become (n,1)\n    return kernelParameter_sigma * np.exp(-0.5 * (1 / kernelParameter_l) * sqdist)","key":"yj4lGMuyC0"},{"type":"output","id":"GHUNXv9vUykQ8pmpMSyDn","data":[],"key":"JINbEAyYcn"}],"data":{"type":"notebook-code","execution":{"iopub.execute_input":"2023-07-20T14:17:11.215207Z","iopub.status.busy":"2023-07-20T14:17:11.214208Z","iopub.status.idle":"2023-07-20T14:17:11.227229Z","shell.execute_reply":"2023-07-20T14:17:11.227229Z"}},"key":"XXIQsyTXVf"},{"type":"block","children":[{"type":"code","lang":"python","executable":true,"value":"K_train = kernel(X_train, X_train)\nK_test = kernel(X_test, X_test)\nK_cross = kernel(X_train, X_test)\n\nL = np.linalg.cholesky(K_train + s * np.eye(n_train))\nLk = np.linalg.solve(L, K_cross)\nmean_prediction = np.dot(Lk.T, np.linalg.solve(L, y_train)).ravel()\nvar_prediction = np.diag(K_test) - np.sum(Lk**2, axis=0)\nstd_prediction = np.sqrt(var_prediction)","key":"RNtFOA4TEk"},{"type":"output","id":"j4YLatY0PD6P0woXkOHxr","data":[],"key":"Pyo1cAB4VP"}],"data":{"type":"notebook-code","execution":{"iopub.execute_input":"2023-07-20T14:17:11.229229Z","iopub.status.busy":"2023-07-20T14:17:11.229229Z","iopub.status.idle":"2023-07-20T14:17:11.243275Z","shell.execute_reply":"2023-07-20T14:17:11.243275Z"}},"key":"IWoAMKSZJU"},{"type":"block","children":[{"type":"code","lang":"python","executable":true,"value":"fig, ax = plt.subplots(figsize=(15, 5))\nax.plot(X_true, y_true, label=latex_func, linestyle=\"dotted\")\nax.scatter(X_train, y_train, label=\"Observations\")\nax.plot(X_test, mean_prediction, label=\"Mean prediction\")\nax.fill_between(\n    X_test.ravel(),\n    mean_prediction - 1.96 * std_prediction,\n    mean_prediction + 1.96 * std_prediction,\n    alpha=0.5,\n    label=r\"95% confidence interval\",\n)\nax.legend()\nax.set_xlabel(\"$x$\")\nax.set_ylabel(\"$f(x)$\")\nax.set_title(\"Gaussian process regression on a noisy dataset\")\nfig.tight_layout()\nfig.savefig(figures_path + \"GaussianProcessRegression.svg\")\nfig.savefig(figures_path + \"GaussianProcessRegression.pdf\")","key":"KAoGsP2kPI"},{"type":"output","id":"jH_xGCYmDcmxndEqMrEOq","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"426f83b97b756c4477875434f3e50f01","path":"/SequentialEGM/build/426f83b97b756c4477875434f3e50f01.png"},"text/plain":{"content":"<Figure size 1500x500 with 1 Axes>","content_type":"text/plain"}}}],"key":"VKGxfzEUio"}],"data":{"type":"notebook-code","execution":{"iopub.execute_input":"2023-07-20T14:17:11.246274Z","iopub.status.busy":"2023-07-20T14:17:11.246274Z","iopub.status.idle":"2023-07-20T14:17:11.663148Z","shell.execute_reply":"2023-07-20T14:17:11.663148Z"}},"key":"w8FVtKMq1Y"},{"type":"block","children":[{"type":"code","lang":"python","executable":true,"value":"L = np.linalg.cholesky(K_test + 1e-6 * np.eye(n_test) - np.dot(Lk.T, Lk))\nf_post = mean_prediction.reshape(-1, 1) + np.dot(L, rng.normal(size=(n_test, 50)))","key":"ayoRTyYaCn"},{"type":"output","id":"PZvBHaOJigWNLtg0ms1T3","data":[],"key":"kp44O1PoZs"}],"data":{"type":"notebook-code","execution":{"iopub.execute_input":"2023-07-20T14:17:11.666150Z","iopub.status.busy":"2023-07-20T14:17:11.666150Z","iopub.status.idle":"2023-07-20T14:17:11.679166Z","shell.execute_reply":"2023-07-20T14:17:11.679166Z"}},"key":"Yr8rnWZdUV"},{"type":"block","children":[{"type":"code","lang":"python","executable":true,"value":"fig, ax = plt.subplots(figsize=(15, 5))\nax.plot(X_true, y_true, label=latex_func, linestyle=\"dotted\")\nax.scatter(X_train, y_train, label=\"Observations\")\nax.plot(X_test, mean_prediction, label=\"Mean prediction\")\nax.plot